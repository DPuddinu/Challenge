{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "trip-of-the-day",
      "urls": [
        "/trips/random/trip-of-the-day"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "1d",
        "maxSize": 1,
        "timeout": "3s"
      }
    },
    {
      "name": "api-trips",
      "urls": [
        "/trips/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "1h",
        "maxSize": 100,
        "timeout": "5s"
      }
    }
  ]
}
